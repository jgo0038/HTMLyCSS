    list-style: none;\r\n    font-size: large;\r\n}\r\n\r\n.categorias>li>a{\r\n    text-decoration: none;\r\n    color:black;\r\n}\r\n\r\n.about{\r\n    display: flex;\r\n    align-items:center;\r\n    justify-content:center;\r\n    flex-direction: column;\r\n}\r\n\r\n#conocenos{\r\n    display: flex;\r\n    text-align: center;\r\n    justify-content: center;\r\n    flex-direction: row;\r\n}\r\n\r\n#txtAbout p{\r\n    color:#D2BF37;\r\n    background-color: #1f1f1f;\r\n    margin:0px;\r\n    padding-top: 10px;\r\n}\r\n\r\n#txtAbout h1{\r\n    margin-top: 80px;\r\n}\r\n\r\n#txtAbout h1 a{\r\n    display: table-cell;\r\n    text-decoration: none;\r\n    color:#D2BF37;\r\n}\r\n\r\n#txtAbout h1 a:hover{\r\n    color:white;\r\n}\r\n\r\n.animales{\r\n    display: flex;\r\n    justify-content: center;\r\n    align-items: center;\r\n}\r\n  \r\n"},"lineCount":null}},"error":null,"hash":"d7a8cc7ad75b8ba8ede6e303898208ef","cacheData":{"env":{}}}